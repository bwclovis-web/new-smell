# Database URLs for migration script
# Copy this file to .env in the project root and fill in your values

# Local PostgreSQL database (source)
LOCAL_DATABASE_URL="postgresql://postgres:password@localhost:5432/new_scent"

# Remote Prisma Accelerate database (destination)
REMOTE_DATABASE_URL="prisma+postgres://accelerate.prisma-data.net/?api_key=YOUR_API_KEY"

# Main database URL used by the application (typically points to local for development)
DATABASE_URL="postgresql://postgres:password@localhost:5432/new_scent"
