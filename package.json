{
  "name": "voodoo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/copy-images.js && react-router build",
    "build:webp": "CONVERT_PNG_TO_WEBP=true node scripts/copy-images.js && react-router build",
    "convert:png-to-webp": "tsx scripts/convert-png-to-webp.js",
    "optimize:images": "node scripts/optimize-images.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "test:compression": "node scripts/test-compression.js",
    "create:component": "node generator/create-component.mjs",
    "db:docs": "npx prisma-docs-generator serve",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:serve": "prisma db pull && prisma db push && run-p db:studio db:docs dev",
    "db:studio": "npx prisma studio",
    "db:push:remote:cli": "node scripts/push_local_to_remote_db_cli.js",
    "db:diagnose:perfumes": "node scripts/diagnose-perfume-mismatch.js",
    "db:check:migration": "node scripts/check-migration-status.js",
    "clean:notes": "node scripts/clean-notes.js",
    "clean:notes:dry-run": "node scripts/clean-notes.js --dry-run",
    "clean:notes:complete": "node scripts/clean-notes-complete.js --dry-run",
    "clean:notes:complete:apply": "node scripts/clean-notes-complete.js --confirm-apply",
    "clean:notes:complete:full": "node scripts/clean-notes-complete.js --confirm-apply --apply-ai --recheck-duplicates",
    "clean:notes:apply-ai": "node scripts/apply-ai-recommendations.js",
    "db:backup": "node scripts/backup-database-prisma.js",
    "db:backup:pg": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database-prisma.js",
    "db:restore:list": "node scripts/restore-database-prisma.js --list",
    "db:pull:production": "node scripts/pull-production-to-local.js",
    "db:restore:pg": "node scripts/restore-database.js",
    "db:restore:pg:list": "node scripts/restore-database.js --list",
    "import:csv": "tsx scripts/import-csv.ts",
    "import:csv-noir": "tsx scripts/import-csv-noir.ts",
    "lint": "eslint --fix --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "dev": "node scripts/copy-images.js && run-p start:server open:localhost",
    "open:localhost": "start http://localhost:2112",
    "start": "run-p start:server",
    "start:server": "node ./api/server.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:performance": "vitest run --config vitest.config.performance.ts",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "deploy": "npx vercel --prod",
    "deploy:preview": "npx vercel",
    "test:ci": "vitest run --config vitest.config.ci.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ci": "playwright test --config=playwright.config.ci.ts",
    "test:e2e:critical": "playwright test --grep=\"critical\"",
    "test:e2e:mobile": "playwright test --grep=\"mobile\"",
    "test:coverage:unit": "vitest run --config vitest.config.unit.ts --coverage",
    "test:coverage:integration": "vitest run --config vitest.config.integration.ts --coverage",
    "test:coverage:all": "npm run test:coverage:unit && npm run test:coverage:integration",
    "test:security": "npm audit --audit-level moderate",
    "test:quality": "npm run lint && npm run typecheck",
    "test:full": "npm run test:quality && npm run test:security && npm run test:all && npm run test:e2e",
    "test:fix": "npm run test:unit -- --run --reporter=verbose",
    "test:debug": "vitest --inspect-brk",
    "test:update-snapshots": "vitest run --update-snapshots",
    "test:clean": "rm -rf test-results coverage .nyc_output",
    "typecheck": "react-router typegen && tsc",
    "postinstall": "node scripts/postinstall.cjs",
    "crews:setup": "cd crews && python -m venv venv && .\\venv\\Scripts\\activate && pip install -r requirements.txt",
    "crews:enrich": "cd crews && .\\venv\\Scripts\\python scripts/run_research_crew.py",
    "ai:enrich": "npm run crews:enrich --",
    "prepare": "husky"
  },
